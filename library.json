{
  "name": "quickjs-ng",
  "description": "QuickJS Javascript Engine for ESP32",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjoerge/quickjs-ng.git"
  },
  "keywords": "JavaScript",
  "authors": [
    {
      "name": "Fabrice Bellard",
      "url": "https://bellard.org/quickjs/"
    }
  ],
  "version": "0.0.1",
  "framework": "*",
  "platforms": "espressif32",
  "build": {
    "flags": "-DCONFIG_VERSION=\\\"2024-08-29\\\"",
    "srcFilter": [
      "+<*.c>",
      "+<*.h>",
      "-<tests/>",
      "-<examples/>",
      "-<doc/>",
      "-<ctest.c>",
      "-<cxxtest.cc>",
      "-<dirent_compat.h>",
      "-<fuzz.c>",
      "-<getopt_compat.h>",
      "-<libregexp-opcode.h>",
      "-<libunicode-table.h>",
      "-<list.h>",
      "-<qjs.c>",
      "-<qjsc.c>",
      "-<quickjs-atom.h>",
      "-<quickjs-c-atomics.h>",
      "-<quickjs-libc.c>",
      "-<quickjs-libc.h>",
      "-<quickjs-opcode.h>",
      "-<run-test262.c>",
      "-<unicode_gen.c>",
      "-<unicode_gen_def.h>"
    ]
  }
}